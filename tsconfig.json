{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "paths": {
      "components/*": ["./src/app/components/*"],
      "constants/*": ["./src/app/constants/*"],
      "enums/*": ["./src/app/enums/*"],
      "interfaces/*": ["./src/app/interfaces/*"],
      "types/*": ["./src/app/types/*"],
      "utils/*": ["./src/app/utils/*"]
    },
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "additional.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "lib/heatmap/*.d.ts",
    "lib/heatmap/fakeImpetus.js",
    "backend/services/topSellingLands.ts",
    "lib/chatbot/MessageParser.js",
    "lib/chatbot/ActionProvider.js",
    "lib/chatbot/MessageParser.js",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
